{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    { "src": "dist/shared/infra/http/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/shared/infra/http/server.js" },
    { "src": "/(.*)", "dest": "/dist/shared/infra/http/server.js" }
  ]
}
